<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".coffee_activity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="90dp"
        android:text="RISARALDA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="90dp"
        android:text="CHIAPAS PASCO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="53dp"
        android:text="MONTECILLOS"
        app:layout_constraintBottom_toTopOf="@+id/risaralda"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/risaralda"
        android:layout_width="160dp"
        android:layout_height="232dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/risaralda" />

    <ImageView
        android:id="@+id/chiapasPasco"
        android:layout_width="160dp"
        android:layout_height="232dp"
        android:layout_marginStart="16dp"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/pasco_perou" />

    <ImageView
        android:id="@+id/montecillos"
        android:layout_width="160dp"
        android:layout_height="232dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/montecillos" />

    <ImageView
        android:id="@+id/chiapas"
        android:layout_width="160dp"
        android:layout_height="232dp"
        android:layout_marginStart="16dp"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/chiapas_mexique" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginBottom="53dp"
        android:text="CHIAPAS"
        app:layout_constraintBottom_toTopOf="@+id/chiapasPasco"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>